{
  "name": "a2z-tsn-frer-system",
  "version": "2.0.0",
  "description": "Autonomous A2Z Gigabit TSN/FRER Network System - Complete Implementation",
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "build": "webpack --mode production",
    "build:mobile": "cd mobile && react-native run-android && react-native run-ios",
    "docker:build": "docker build -t a2z-tsn .",
    "docker:run": "docker run -p 3000:3000 a2z-tsn",
    "docs": "jsdoc -c jsdoc.json",
    "validate": "npm run lint && npm run test",
    "deploy": "npm run validate && npm run build && npm run docker:build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/autonomous-a2z/tsn-frer-system.git"
  },
  "keywords": [
    "TSN",
    "FRER",
    "802.1CB",
    "autonomous-vehicles",
    "gigabit-ethernet",
    "microchip",
    "LAN9692",
    "LAN9662",
    "real-time",
    "network",
    "automotive"
  ],
  "author": "Autonomous A2Z Engineering Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.6.1",
    "axios": "^1.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.72.6",
    "react-native-chart-kit": "^6.12.0",
    "react-native-svg": "^13.14.0",
    "d3": "^7.8.5",
    "chart.js": "^4.4.0",
    "mqtt": "^5.0.5",
    "node-opcua": "^2.115.0",
    "serialport": "^12.0.0",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.1",
    "redis": "^4.6.10",
    "influxdb-client": "^1.33.2",
    "prometheus-api-metrics": "^3.2.2",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "body-parser": "^1.20.2"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-native": "^0.72.5",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.1.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "nodemon": "^3.0.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "babel-loader": "^9.1.3",
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "jsdoc": "^4.0.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run validate"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}